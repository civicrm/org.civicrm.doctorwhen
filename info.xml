<?xml version="1.0"?>
<extension key="org.civicrm.doctorwhen" type="module">
  <file>doctorwhen</file>
  <name>Doctor When</name>
  <description>Analyze and fix discrepancies in timestamps and datetimes</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Tim Otten</author>
    <email>totten@civicrm.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/civicrm/org.civicrm.doctorwhen</url>
    <url desc="Support">https://chat.civicrm.org</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-08-29</releaseDate>
  <version>1.2</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.60</ver>
  </compatibility>
  <civix>
    <namespace>CRM/DoctorWhen</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
